##############################################################################################################
include ../MAKE_VARIABLES_COMMON.mk
##############################################################################################################
## D E F I N I T I O N   N A M E   E X E C U T B L E   C O D E ###############################################
### EXE=${JACOBIAN}_${ODE_MODEL}
EXE=${ODE_MODEL}
##############################################################################################################
EXTERN=#Possibilities: void (to be locally executed) / -static (to be non-locally executed) #
######################################## Possibilities: GA (locally executed) / GA_EXTERN (non-locally executed)
LDFLAGS+=-lm -lgsl -lgslcblas -lDa_GSL_UTL -lNumRec -lNRUTL -lda_cpgplot_XY_GRID_FILE -lda_cpgplot_BASIC ${EXTRA_LIBRARY_PATH}
CXXFLAGS+=${EXTRA_INCLUDE_PATH} -D${V} -D${CPG} -D${FILE}  -D${TYPE_NUMERICAL_INTEGRATION} -D${RANDOM_SEED} -D${H_MODEL} -D${M_MODEL} -D${ODEMODEL} -D${S} -D${Q} -D${ERROR}  -D${JACOBIAN} -D${NR} -DHAVE_INLINE ${EXTERN} -g # #-Wall
##############################################################################################################
# either -g or some optimization opiton, for instance, -O3
# This makefile uses full power of conditional compilation. There is a bunch of variables that control this and
# are passed as flags -D[NAME_of_the_VARIABLE] to the compiler during compilation
##############################################################################################################
LIBS=-L/usr/lib -lX11
PGPLOT_LIB=-L/usr/lib -lpgplot -L/usr/lib -lpng
CPGPLOT_LIB=-L/usr/lib -lcpgplot -lpgplot -L/usr/lib -lpng
##############################################################################################################
OBJECTS = main.o report.o argumentControl.o MODEL.o MODEL_STO.o                                                                                                                                                                                                                                                       \
	Parameter_Model.o Time_Control.o Trend_Control.o Community.o Parameter_Table.o values_Parameter_Model.o definition_OutPut_Variables.o assignCodes_to_Model_Parameters.o assignLabel_to_Model_Parameters.o assignLabel_to_Model_Variables.o assignLabel_to_Output_Variables.o assignSymbol_to_Model_Parameters.o assignSymbol_to_Model_Variables.o assignSymbol_to_Output_Variables.o assignVectorEntry_to_Structure.o assignStructValue_to_VectorEntry.o fPRINT_Input_Parameters.o fPRINT_Output_Variables.o                                                                                                                        													    \
	Latex_assignLabel_to_Model_Parameters.o  Latex_Write_Parameter_Table.o                         \
	Parameter_Space.o index.Definition${TYPE_of_INDEX}.o index.Definition_Full_Parameter_List.o boundary_Model_Parameter.o  boundary_Initial_Condition.o                                                                                                                                                          \
	superinfection_Functions.o beta_Functions.o                                                    \
	deterministic_time_dynamics.o numerical_Integration_Driver.o Initial_Conditions_Numerical_Integration.o Initial_Condition_from_Parameter_Table.o  Random_Initial_Condition.o ODE_sys_${ODE_MODEL}.o JAC_sys_${ODE_MODEL}.o JACOBIAN_matrix_${ODE_MODEL}.o                                                     \
	Stochastic_Time_Dynamics.o Advance_Current_Time.o  Choose_Village.o  Initial_Conditions_Stochastic_Dynamics.o  Patch_System_Initialization.o  Execute_One_Step.o  Temporal_Dynamics.o  Temporal_Dynamics_Update.o                                                                                             \
	model_Variables_Code.o Error_Function.o  R_0_Vectorial_Capacity.o Total_Population_Numbers.o  Total_Population_Fractions.o Total_Population_Densities.o  \
	stat.o                                                                                         \
	CPGPLOT___X_Y___Parameter_Table.o                                                              \
	Feasibility_Condition_Humans.o Feasibility_Condition_Mosquitoes.o Fixed_Points_All.o Fixed_Points_All_Intersection.o W_y_Fixed_Points_Functions.o intermediate_Root.o y_W_diagramm.o Stable_Solution.o Stationary_y_W_Functions.o Stable_Solution_Partial_Mosquito.o Stable_Solution_Total_Mosquito.o  

CC=gcc##
FF=gfortran##

all: ${EXE} COMPILATION_OPTIONS

${EXE}:  	$(OBJECTS)
		$(FF) -o ${EXE} $(OBJECTS) $(LDFLAGS) $(CXXFLAGS) $(CPGPLOT_LIB) $(LIBS)

# da__qag.o:	/home/dalonso/GSL_v1.12/gsl-1.12/integration/da__qag.c
#		${CC} -c /home/dalonso/GSL_v1.12/gsl-1.12/integration/da__qag.c $(CXXFLAGS)

# Main program functions:
main.o: main.c ${HEAD.H}
	$(CC) -c main.c $(CXXFLAGS)

